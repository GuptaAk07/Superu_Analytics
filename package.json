{
  "name": "langfuse",
  "version": "2.7.0",
  "scripts": {
    "build": "dotenv -e .env -- turbo run build",
    "infra:dev:up": "docker-compose -f ./docker-compose.dev.yml up -d",
    "infra:dev:down": "docker-compose -f ./docker-compose.dev.yml down",
    "dev": "dotenv -e .env -- turbo run dev",
    "lint": "dotenv -e .env -- turbo run lint",
    "lint:fix": "dotenv -e .env -- turbo run lint:fix",
    "test": "dotenv -e .env -- turbo run test",
    "dx": "dotenv -e .env -- yarn i && yarn run infra:dev:up && cd web && yarn run db:reset && yarn run db:seed:examples && cd .. && yarn run dev",
    "nuke": "bash ./scripts/nuke.sh"
  },
  "workspaces": [
    "worker",
    "web",
    "packages/*"
  ],
  "private": true,
  "license": "MIT",
  "devDependencies": {
    "@types/eslint": "^8.56.2",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "dotenv-cli": "^7.4.1",
    "eslint": "^8.56.0",
    "prettier": "^3.2.5",
    "turbo": "^1.12.5"
  },
  "repository": "https://github.com/langfuse/langfuse.git",
  "packageManager": "yarn@1.22.1"
}
